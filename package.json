{
  "name": "@firebaseui/ng-bootstrap",
  "description": "Angular Bootstrap UI library for firebase authentication powered by @ng-bootstrap",
  "version": "0.3.0",
  "homepage": "https://github.com/firebaseui/ng-bootstrap",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/AnthonyNahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/firebaseui/ng-bootstrap.git"
  },
  "license": "MIT",
  "keywords": [
    "ng",
    "angular",
    "library",
    "bootstrap",
    "ng-bootstrap",
    "firebase",
    "firebase authentication",
    "sign in",
    "sign up",
    "register",
    "forgot password",
    "email confirmation",
    "sign with google facebook twitter githbub"
  ],
  "bugs": {
    "url": "https://github.com/firebaseui/ng-bootstrap/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=firebaseui/ng-bootstrap --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@toverux/ngx-sweetalert2": "^4.0.0",
    "angularfire2": "5.0.0-rc.12",
    "firebase": "^5.3.1",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@angular/compiler": "^6.1.2",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/platform-server": "^6.1.2",
    "@compodoc/gulp-compodoc": "^0.0.9",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.8",
    "angular-cli-ghpages": "^0.5.3",
    "angular2-template-loader": "^0.6.2",
    "ansi-colors": "^2.0.5",
    "autoprefixer": "^9.1.1",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^4.4.3",
    "commitplease": "^3.2.0",
    "conventional-github-releaser": "2.0.2",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "del": "3.0.0",
    "fancy-log": "1.3.2",
    "greenkeeper-lockfile": "^1.15.1",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-conventional-changelog": "^2.0.1",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "^2.8.0",
    "gulp-hub": "0.8.0",
    "gulp-inline-ng2-template": "4.1.0",
    "gulp-tslint": "8.1.3",
    "html-loader": "0.5.5",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jest-preset-angular": "^5.2.3",
    "lodash": "4.17.10",
    "node-sass": "^4.9.3",
    "postcss": "^7.0.2",
    "postcss-strip-inline-comments": "0.1.5",
    "pump": "3.0.0",
    "raw-loader": "0.5.1",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.1",
    "rxjs": "^6.2.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "0.2.3",
    "to-string-loader": "1.1.5",
    "travis-status": "^3.0.1",
    "tslint": "^5.11.0",
    "typescript": "2.7.2",
    "yargs": "12.0.1",
    "zone.js": "0.8.26"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@ng-bootstrap/ng-bootstrap",
      "font-awesome",
      "@types/jasmine",
      "@types/node",
      "gulp-inline-ng2-template",
      "conventional-github-releaser",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
